ServerName "$CLOUDWAY_APP_DNS"
ServerRoot "$CLOUDWAY_PHPMYADMIN_DIR"

Listen $CLOUDWAY_PHPMYADMIN_HOST:$CLOUDWAY_PHPMYADMIN_PORT
User $CLOUDWAY_APP_ID
Group $CLOUDWAY_APP_ID

ErrorLog "|/usr/sbin/rotatelogs $CLOUDWAY_PHPMYADMIN_DIR/logs/error_log-%Y%m%d-%H%M%S-%Z 86400"
CustomLog "|/usr/sbin/rotatelogs $CLOUDWAY_PHPMYADMIN_DIR/logs/access_log-%Y%m%d-%H%M%s-%Z 86400" combined

<IfVersion >= 2.4>
DefaultRuntimeDir "$CLOUDWAY_PHPMYADMIN_DIR/run"
</IfVersion>

php_value include_path "."
