<?xml version="1.0" ?>

<plugin>
  <name>cwas</name>
  <display-name>Cloudway Web Application Server 1.1</display-name>
  <version>1.1</version>
  <vendor>cloudway</vendor>
  <plugin-version>1.0</plugin-version>
  <plugin-vendor>cloudway</plugin-vendor>
  <category>Framework</category>
  <endpoint>
    <private-host-name>HOST</private-host-name>
    <private-port-name>PORT</private-port-name>
    <private-port>8080</private-port>
    <proxy-mapping>
      <frontend>/</frontend>
      <backend>/</backend>
      <protocols>ajp,http</protocols>
    </proxy-mapping>
    <proxy-mapping>
      <frontend>/cloudway/mux</frontend>
      <backend>/cloudway/mux</backend>
      <protocols>ws</protocols>
    </proxy-mapping>
  </endpoint>
</plugin>
